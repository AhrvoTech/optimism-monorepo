  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.3",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
