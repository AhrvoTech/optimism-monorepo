  "version": "0.0.1-alpha.1",
  "description": "OVM fraud proving library",
  "main": "build/index.js",
  "files": [
    "build/**/*.js"
  ],
  "workspaces": {
    "nohoist": [
      "**/@nomiclabs",
      "**/@nomiclabs/**",
      "**/solidity-coverage",
      "**/solidity-coverage/**",
      "**/typescript",
      "**/typescript/**",
      "**/ts-node",
      "**/ts-node/**"
    ]
  },
  "scripts": {
    "all": "yarn clean && yarn build && yarn test && yarn fix && yarn lint",
    "test": "yarn run test:typescript",
    "test:typescript": "buidler test --show-stack-traces",
    "lint": "yarn run lint:typescript",
    "lint:typescript": "tslint --format stylish --project .",
    "fix": "prettier --config ../../prettier-config.json --write \"index.ts\" \"{src,test}/**/*.ts\"",
    "build": "yarn run build:typescript",
    "build:typescript": "tsc -p .",
    "clean": "rimraf build/"
  },
